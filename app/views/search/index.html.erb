<!DOCTYPE html>
<html>
<table class="table table-striped">

  <thead>
  <tr>
    <th>Value</th>
    <th>Type</th>
    <th>Source</th>
    <th>Tags</th>
  </tr>
  </thead>
  <tbody>
  <%= @results.all.each do |i| %>
      <tr>
        <td><%= i.value %></td>
        <td><%= i.kind %></td>
        <td><%= i.source %></td>
        <td><%= i.tags %></td>
      </tr>
  <% end %>
  </tbody>
</table>
</html>